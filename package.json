{
  "name": "quicktypes",
  "version": "1.0.0",
  "description": "QuickBase type generation and client library with Payload CMS-inspired API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client/types": {
      "import": "./dist/client/types.js",
      "require": "./dist/client/types.js", 
      "types": "./dist/client/types.d.ts"
    },
    "./scripts/generate": {
      "import": "./dist/scripts/generate.js",
      "require": "./dist/scripts/generate.js",
      "types": "./dist/scripts/generate.d.ts"
    }
  },
  "bin": {
    "quicktypes-generate": "./dist/scripts/generate.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test:example": "npx tsx example-usage.ts",
    "test:consuming": "npx tsx example-consuming-project.ts"
  },
  "files": [
    "dist/",
    "src/",
    "types/",
    "scripts/",
    "README.md"
  ],
  "keywords": [
    "quickbase", 
    "typescript", 
    "code-generation", 
    "api-client",
    "type-safety",
    "payload-cms"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "json-schema-to-typescript": "^15.0.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2"
  }
}
