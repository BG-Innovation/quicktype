{
  "name": "quicktype",
  "version": "1.0.0",
  "description": "QuickBase type generation and discovery tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "quicktype": "dist/scripts/generate.cjs"
  },
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "dev": "npm run generate",
    "types": "npm run generate",
    "example": "npx tsx example-usage.ts",
    "example:consumer": "npx tsx example-consumer-usage.ts",
    "build": "rm -rf dist && tsc && tsc -p tsconfig.scripts.json && mv dist/scripts/generate.js dist/scripts/generate.cjs",
    "test": "npm run build && npm run example:consumer",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["quickbase", "typescript", "code-generation", "payload-inspired"],
  "author": "",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "json-schema-to-typescript": "^15.0.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2"
  }
}
